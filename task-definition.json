{
    "family": "omer-kisisel-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::522354331614:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "omer-kisisel-container",
        "image": "IMAGE_URI",
        "portMappings": [
          {
            "containerPort": 8080,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
  }